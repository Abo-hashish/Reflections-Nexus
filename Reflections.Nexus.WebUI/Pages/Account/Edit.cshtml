@page
@model Reflections.Nexus.WebUI.Pages.Account.EditModel

        @{
        ViewData["Title"] = "Edit";
        ViewData["Entity"] = "Users";
        ViewData["BreadCrumb"] = "<a href='/Users' class='breadcrumb-item'>Users</a>";
        }
        

 <form method="post">
    <div class="card">
        <div class="card-header">
             <h5 class="card-title mb-0">Edit ApplicationUser</h5>
        </div>
        <div class="card-body">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row">
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.FirstName" class="control-label"></label>
                                    <input asp-for="ApplicationUser.FirstName" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.FirstName" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.LastName" class="control-label"></label>
                                    <input asp-for="ApplicationUser.LastName" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.LastName" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group mt-3">
                                    <label class="form-check-label">
                                        <input class="form-check-input" asp-for="ApplicationUser.IsActive" /> @Html.DisplayNameFor(model => model.ApplicationUser.IsActive)
                                    </label>
                                </div>
                            </div>
                       
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.UserName" class="control-label"></label>
                                    <input asp-for="ApplicationUser.UserName" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.UserName" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.NormalizedUserName" class="control-label"></label>
                                    <input asp-for="ApplicationUser.NormalizedUserName" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.NormalizedUserName" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.Email" class="control-label"></label>
                                    <input asp-for="ApplicationUser.Email" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.Email" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.NormalizedEmail" class="control-label"></label>
                                    <input asp-for="ApplicationUser.NormalizedEmail" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.NormalizedEmail" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group mt-3">
                                    <label class="form-check-label">
                                        <input class="form-check-input" asp-for="ApplicationUser.EmailConfirmed" /> @Html.DisplayNameFor(model => model.ApplicationUser.EmailConfirmed)
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.PasswordHash" class="control-label"></label>
                                    <input asp-for="ApplicationUser.PasswordHash" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.PasswordHash" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.SecurityStamp" class="control-label"></label>
                                    <input asp-for="ApplicationUser.SecurityStamp" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.SecurityStamp" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.ConcurrencyStamp" class="control-label"></label>
                                    <input asp-for="ApplicationUser.ConcurrencyStamp" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.ConcurrencyStamp" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.PhoneNumber" class="control-label"></label>
                                    <input asp-for="ApplicationUser.PhoneNumber" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.PhoneNumber" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group mt-3">
                                    <label class="form-check-label">
                                        <input class="form-check-input" asp-for="ApplicationUser.PhoneNumberConfirmed" /> @Html.DisplayNameFor(model => model.ApplicationUser.PhoneNumberConfirmed)
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group mt-3">
                                    <label class="form-check-label">
                                        <input class="form-check-input" asp-for="ApplicationUser.TwoFactorEnabled" /> @Html.DisplayNameFor(model => model.ApplicationUser.TwoFactorEnabled)
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.LockoutEnd" class="control-label"></label>
                                    <input asp-for="ApplicationUser.LockoutEnd" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.LockoutEnd" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group mt-3">
                                    <label class="form-check-label">
                                        <input class="form-check-input" asp-for="ApplicationUser.LockoutEnabled" /> @Html.DisplayNameFor(model => model.ApplicationUser.LockoutEnabled)
                                    </label>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label asp-for="ApplicationUser.AccessFailedCount" class="control-label"></label>
                                    <input asp-for="ApplicationUser.AccessFailedCount" class="form-control" />
                                    <span asp-validation-for="ApplicationUser.AccessFailedCount" class="text-danger"></span>
                                </div>
                            </div>
            </div>
        </div>
        <div class="card-footer d-flex justify-content-between align-items-center bg-light border-top-0">
            <a asp-page="Index" class="btn btn-primary btn-labeled btn-labeled-start">
                Cancel
                <span class="btn-labeled-icon bg-black bg-opacity-20">
                    <i class="icon-arrow-left8"></i>
                </span>
            </a>
            <button type="submit" class="btn btn-primary btn-labeled btn-labeled-end">
                Save
                <span class="btn-labeled-icon bg-black bg-opacity-20">
                    <i class="icon-paperplane"></i>
                </span>
            </button>
        </div>
    </div>
</form>

        @section Scripts {
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }

