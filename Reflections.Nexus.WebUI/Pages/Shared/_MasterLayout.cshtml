<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />

    <title>@ViewData["Title"] - Reflections.Nexus.WebUI</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    @* <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" /> *@

    <!-- Global stylesheets -->
    <link href="~/fonts/inter/inter.css" rel="stylesheet" type="text/css">
    
    <link href="~/icons/phosphor/styles.min.css" rel="stylesheet" type="text/css">
    <link href="~/icons/fontawesome/styles.min.css" rel="stylesheet" />
    <link href="~/icons/icomoon/styles.min.css" rel="stylesheet" />
    <link href="~/icons/material/styles.min.css" rel="stylesheet" />
    
    <link href="~/css/ltr/all.min.css" id="stylesheet" rel="stylesheet" type="text/css">
    <!-- /global stylesheets -->

    
    <link href="~/js/vendor/tables/datatables/datatables.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/Reflections.Nexus.WebUI.styles.css" asp-append-version="true" />

    @await RenderSectionAsync("MainStyle", required: false)

    <!-- Core JS files -->
    <script src="~/js/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="~/js/jquery/jquery-3.7.1.min.js"></script>
    @* <script src="~/js/demo/demo_configurator.js"></script> *@
    <!-- /core JS files -->
    <!-- Theme JS files -->
    @* <script src="~/js/vendor/visualization/d3/d3.min.js"></script>
    <script src="~/js/vendor/visualization/d3/d3_tooltip.js"></script> *@

    <script src="~/js/app.js"></script>

    @* <script src="~/js/demo/pages/dashboard.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/streamgraph.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/sparklines.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/lines.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/areas.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/donuts.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/bars.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/progress.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/heatmaps.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/pies.js"></script>
    <script src="~/js/demo/charts/pages/dashboard/bullets.js"></script> *@
    <!-- /theme JS files -->

    <script src="~/js/vendor/tables/datatables/datatables.min.js"></script>
    @* <script src="~/js/vendor/tables/datatables/extensions/pdfmake/pdfmake.min.js"></script>
    <script src="~/js/vendor/tables/datatables/extensions/pdfmake/vfs_fonts.min.js"></script>
    <script src="~/js/vendor/tables/datatables/extensions/buttons.min.js"></script> *@


    <script src="~/js/custom.js"></script>
    @await RenderSectionAsync("MainScript", required: false)

</head>
<body>

    <partial name="_MainNav" />

    <!-- Page content -->
    <div class="page-content">


        @RenderBody()

    </div>
    <!-- /page content -->
    

</body>
</html>